---
layout: none
---
[
{% for post in site.posts %}
  {
    "title": "{% if post.title and post.title != '' %}{{ post.title | escape }}{% else %}{{ post.content | strip_html | split: '.' | first | strip | escape }}{% endif %}",
    "url": "{{ post.url | relative_url }}",
    "date": "{{ post.date | date: '%Y-%m-%d' }}",
    "content": "{{ post.content | strip_html | split: '.' | first | strip | escape }}"
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
